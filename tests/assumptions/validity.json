      "function": "AvgSpread",
      "inputs": {
        "x": ["NaN", 2.0, 3.0],
        "y": [1.0, 2.0, 3.0]
      },
      "expected_violation": {
        "id": "validity"
      }
    },
    {
      "name": "disparity_empty_x",
      "function": "Disparity",
      "inputs": {
        "x": [],
        "y": [1.0, 2.0, 3.0]
      },
      "expected_violation": {
        "id": "validity"
      }
    },
    {
      "name": "disparity_inf_y",
      "function": "Disparity",
      "inputs": {
        "x": [1.0, 2.0, 3.0],
        "y": ["Infinity"]
      },
      "expected_violation": {
        "id": "validity"
      }
    }
  ]
}
