{
  "suite": "validity",
  "description": "Tests for the validity assumption: non-empty input with finite defined real values",
  "cases": [
    {
      "name": "ratio_empty_x",
      "function": "Ratio",
      "inputs": {
        "x": [],
        "y": [1.0, 2.0, 3.0]
      },
      "expected_violation": {
        "id": "validity"
      }
    },
    {
      "name": "ratio_empty_y",
      "function": "Ratio",
      "inputs": {
        "x": [1.0, 2.0, 3.0],
        "y": []
      },
      "expected_violation": {
        "id": "validity"
      }
    },
    {
      "name": "ratio_nan_x",
      "function": "Ratio",
      "inputs": {
        "x": [1.0, "NaN", 3.0],
        "y": [1.0, 2.0, 3.0]
      },
      "expected_violation": {
        "id": "validity"
      }
    },
    {
      "name": "ratio_nan_y",
      "function": "Ratio",
      "inputs": {
        "x": [1.0, 2.0, 3.0],
        "y": [1.0, "NaN", 3.0]
      },
      "expected_violation": {
        "id": "validity"
      }
    },
    {
      "name": "ratio_inf_x",
      "function": "Ratio",
      "inputs": {
        "x": [1.0, "Infinity", 3.0],
        "y": [1.0, 2.0, 3.0]
      },
      "expected_violation": {
        "id": "validity"
      }
    },
    {
      "name": "ratio_neg_inf_y",
      "function": "Ratio",
      "inputs": {
        "x": [1.0, 2.0, 3.0],
        "y": [1.0, "-Infinity", 3.0]
      },
      "expected_violation": {
        "id": "validity"
      }
    },
    {
      "name": "center_empty",
      "function": "Center",
      "inputs": {
        "x": []
      },
      "expected_violation": {
        "id": "validity"
      }
    },
    {
      "name": "center_nan",
      "function": "Center",
      "inputs": {
        "x": [1.0, "NaN", 3.0]
      },
      "expected_violation": {
        "id": "validity"
      }
    },
    {
      "name": "shift_empty_x",
      "function": "Shift",
      "inputs": {
        "x": [],
        "y": [1.0, 2.0, 3.0]
      },
      "expected_violation": {
        "id": "validity"
      }
    },
    {
      "name": "shift_empty_y",
      "function": "Shift",
      "inputs": {
        "x": [1.0, 2.0, 3.0],
        "y": []
      },
      "expected_violation": {
        "id": "validity"
      }
    },
    {
      "name": "shift_nan_x",
      "function": "Shift",
      "inputs": {
        "x": [1.0, "NaN", 3.0],
        "y": [1.0, 2.0, 3.0]
      },
      "expected_violation": {
        "id": "validity"
      }
    },
    {
      "name": "shift_nan_y",
      "function": "Shift",
      "inputs": {
        "x": [1.0, 2.0, 3.0],
        "y": [1.0, "NaN", 3.0]
      },
      "expected_violation": {
        "id": "validity"
      }
    },
    {
      "name": "rel_spread_empty",
      "function": "RelSpread",
      "inputs": {
        "x": []
      },
      "expected_violation": {
        "id": "validity"
      }
    },
    {
      "name": "rel_spread_nan",
      "function": "RelSpread",
      "inputs": {
        "x": [1.0, "NaN", 3.0]
      },
      "expected_violation": {
        "id": "validity"
      }
    },
    {
      "name": "spread_nan",
      "function": "Spread",
      "inputs": {
        "x": [1.0, "NaN", 3.0]
      },
      "expected_violation": {
        "id": "validity"
      }
    },
    {
      "name": "spread_inf",
      "function": "Spread",
      "inputs": {
        "x": [1.0, "Infinity", 3.0]
      },
      "expected_violation": {
        "id": "validity"
      }
    },
    {
      "name": "avg_spread_empty_x",
      "function": "AvgSpread",
      "inputs": {
        "x": [],
